    "symfony/options-resolver": "5.3.*",
    "symfony/process": "5.3.*",
    "symfony/property-access": "5.3.7",
    "symfony/property-info": "5.3.7",
    "symfony/proxy-manager-bridge": "5.3.*",
    "symfony/routing": "^5.3.7",
    "symfony/security-bundle": "5.3.*",
    "symfony/security-core": "5.3.*",
    "symfony/security-csrf": "5.4.x-dev",
    "symfony/security-guard": "5.3.*",
    "symfony/security-http": "5.3.*",
    "symfony/serializer": "5.3.4",
    "symfony/string": "5.3.7",
    "symfony/templating": "5.3.*",
    "symfony/translation": "^5.3.0",
    "symfony/twig-bridge": "5.3.*",
    "symfony/twig-bundle": "5.0.*",
    "symfony/uid": "5.3.*",
    "symfony/validator": "5.3.*",
    "symfony/web-link": "5.3.4",
    "symfony/webpack-encore-bundle": "^1.12",
    "symfony/workflow": "5.3.*",
    "symfony/yaml": "5.3.*",
    "symfonycasts/verify-email-bundle": "^1.5",
    "tgalopin/html-sanitizer-bundle": "dev-master",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "symfony/browser-kit": "5.3.*",
    "symfony/css-selector": "5.3.*",
    "symfony/debug-bundle": "5.3.*",
    "symfony/maker-bundle": "^1.34",
    "symfony/phpunit-bridge": "*",
    "symfony/stopwatch": "5.3.*",
    "symfony/var-dumper": "5.3.*",
    "symfony/web-profiler-bundle": "5.3.5"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*"
  },
  "scripts": {
    "auto-scripts": {
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": "true",
      "require": "5.0.*"
    }
  }
}
