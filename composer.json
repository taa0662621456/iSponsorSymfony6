{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "2.6.*",
        "cocur/slugify": "^4.0",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "1.13.*",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "easycorp/easyadmin-bundle": "^4.1",
        "google/apiclient": "2.12.*",
        "google/recaptcha": "^1.2",
        "hwi/oauth-bundle": "2.0.0-BETA2",
        "karser/karser-recaptcha3-bundle": "^0.1.20",
        "liip/imagine-bundle": "^2.7.2",
        "nelmio/cors-bundle": "^2.2",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.24",
        "presta/sitemap-bundle": "^3.1",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/event-dispatcher": "6.0.*",
        "symfony/flex": "1.18.*",
        "symfony/framework-bundle": "6.0.*",
        "symfony/google-mailer": "6.0.*",
        "symfony/http-client": "6.0.*",
        "symfony/http-kernel": "6.0.*",
        "symfony/lock": "6.0.*",
        "symfony/mailer": "6.0.*",
        "symfony/notifier": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/rate-limiter": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/security-core": "6.0.*",
        "symfony/security-csrf": "6.0.*",
        "symfony/security-http": "6.0.*",
        "symfony/telegram-notifier": "6.0.*",
        "symfony/translation": "6.0.*",
        "symfony/twig-bridge": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/twilio-notifier": "6.0.*",
        "symfony/ux-turbo": "1.3",
        "symfony/validator": "6.0.*",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "6.0.*",
        "symfonycasts/verify-email-bundle": "^v1.7.0",
        "twig/extra-bundle": "^3.3.0",
        "twig/intl-extra": "^3.3",
        "twig/string-extra": "^3.3",
        "twig/twig": "^3.3.0",
        "twilio/sdk": "^6.31",
        "vich/uploader-bundle": "^1.18"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "rector/rector": "^0.12.16",
        "symfony/debug-bundle": "6.0.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/stopwatch": "6.0.*",
        "symfony/web-profiler-bundle": "6.0.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
