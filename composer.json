  "description": "iSponsor Platform Application",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0",
    "ext-ctype": "*",
    "ext-gmp": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-pdo_pgsql": "*",
    "ext-pdo_sqlite": "*",
    "api-platform/core": "2.6.x-dev",
    "cocur/slugify": "*",
    "composer/package-versions-deprecated": "1.11.99.3",
    "doctrine/annotations": "1.14.x-dev",
    "doctrine/cache": "1.11.3",
    "doctrine/doctrine-bundle": "*",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "2.9.3",
    "easycorp/easyadmin-bundle": "^3.5",
    "erusev/parsedown": "^1.7",
    "google/recaptcha": "^1.2",
    "lcobucci/jwt": "^4.1",
    "lexik/jwt-authentication-bundle": "^2.13",
    "nelmio/cors-bundle": "^2.1",
    "phpdocumentor/reflection-docblock": "^5.2",
    "sensio/framework-extra-bundle": "^6.2",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "5.3.*",
    "symfony/cache": "^5.2 || ^6.0@dev",
    "symfony/config": "5.3.4",
    "symfony/console": "5.3.*",
    "symfony/dependency-injection": "5.3.7",
    "symfony/doctrine-bridge": "5.4.x-dev",
    "symfony/dotenv": "5.3.*",
    "symfony/event-dispatcher": "5.3.*",
    "symfony/expression-language": "5.3.*",
    "symfony/filesystem": "5.3.*",
    "symfony/flex": "*",
    "symfony/form": "^5.4.x-dev",
    "symfony/framework-bundle": "5.3.x-dev",
    "symfony/google-mailer": "5.0.*",
    "symfony/http-client": "5.3.*",
    "symfony/http-foundation": "5.3.7",
    "symfony/http-kernel": "5.3.x-dev",
    "symfony/intl": "5.0.*",
    "symfony/mailer": "5.3.4",
    "symfony/mercure": "^0.5.3",
    "symfony/mercure-bundle": "^0.3.2",
    "symfony/messenger": "5.3.*",
    "symfony/mime": "5.3.7",
