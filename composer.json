{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "2.6.x-dev",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "1.14.x-dev",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "easycorp/easyadmin-bundle": "3.0.x-dev",
        "twig/extra-bundle": "^3.3.0",
        "twig/twig": "^3.3.0",
        "symfony/http-kernel": "5.4.x-dev",
        "symfony/event-dispatcher": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.3.*",
        "symfony/proxy-manager-bridge": "5.3.*",
        "symfony/runtime": "5.3.*",
        "symfony/yaml": "5.3.*",
        "symfony/security-core": "5.4.x-dev",
        "symfony/security-http": "5.4.x-dev",
        "symfony/security-bundle": "5.4.x-dev",
        "symfony/validator": "5.4.x-dev",
        "symfony/translation": "5.4.x-dev",
        "symfony/twig-bridge": "5.3.*",
        "symfony/twig-bundle": "5.0.*",
        "symfony/mailer": "5.4.x-dev"

    },
    "require-dev": {
        "symfony/debug-bundle": "5.3.*",
        "symfony/maker-bundle": "^1.34",
        "symfony/monolog-bundle": "^3.0",
        "symfony/stopwatch": "5.3.*",
        "symfony/web-profiler-bundle": "5.3.*",
        "symfony/webpack-encore-bundle": "^v1.12.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
