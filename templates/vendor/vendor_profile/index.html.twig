                                 <div class="col-2 x1y2"></div>
                                 <div class="col-8 x2y2"></div>
                                 <div class="col-2 x3y2"></div>
                             </div>
                             <div class="row">
                                 <div class="col-2 x1y1"><img src="/{{ asset('cover.jpg') }}" class="rounded float-left img-thumbnail avatar" alt="Avatar"></div>
                                 <div class="col-8 x2y1"></div>
                                 <div class="col-2 x3y1"></div>
                             </div>
                             {% block article_header %}{% endblock %}
